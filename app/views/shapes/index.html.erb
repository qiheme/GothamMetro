<h1>Listing shapes</h1>

<table>
  <thead>
    <tr>
      <th>Dist traveled</th>
      <th>Shape</th>
      <th>Pt lat</th>
      <th>Pt lon</th>
      <th>Pt sequence</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shapes.each do |shape| %>
      <tr>
        <td><%= shape.dist_traveled %></td>
        <td><%= shape.shape_id %></td>
        <td><%= shape.pt_lat %></td>
        <td><%= shape.pt_lon %></td>
        <td><%= shape.pt_sequence %></td>
        <td><%= link_to 'Show', shape %></td>
        <td><%= link_to 'Edit', edit_shape_path(shape) %></td>
        <td><%= link_to 'Destroy', shape, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shape', new_shape_path %>
